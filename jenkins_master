pipeline {
    agent any

    stages {
        
        stage('Welcome to master'){
            steps{
                echo 'Start Program'
            }
        }

        stage('Read file'){
            steps {
                def props = readJSON file: '/data.json'
                    assert props['attr1'] == 'One'
                    assert props.attr1 == 'One'
            }
        }

        stage('Tigger the Slave Jobs') {
            steps {
                echo 'Start Trigger to send name and age to slave'
                build job: 'slave', parameters: [string(name: 'NAME', value: 'Alice'), string(name: 'Ages', value: '21')]
            }
        }
    }
}