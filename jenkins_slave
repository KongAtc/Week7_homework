
pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'coco', description: 'Enter your name')
        string(name: 'Ages', defaultValue: 21, description: 'Enter your age')
    }
    stages {
        stage('Example') {
            steps {
                echo "Hello from Slave ${params.NAME} of ${params.Ages}"
                sh "python myscript.py ${params.NAME} --ages ${params.Ages}"
                
            }
        }
    }
}